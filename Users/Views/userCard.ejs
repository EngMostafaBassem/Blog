
<% for( let i = 0; i < blogs.length; i++ ) { %>
    <div class="col-auto col-md-4 mb-5">


        <div class="card " style="width: 18rem;">
            <div class="card-header">
               <%= blogs[i].blogTitle %> 
            </div>
            <div class="card-body">
              
              <p class="card-text"> <%= blogs[i].blogDescription %> </p>
              <a href='/blogDetail/<%= blogs[i].blogID %>' class="btn btn-primary">Read More</a>
            </div>
        
            <div class="card-footer">
                <p><%= blogs[i].blogDate.toISOString() %> </p>
             </div>
        
          </div>
        
        </div>
<% } %>
